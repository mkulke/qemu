# See docs/devel/tracing.rst for syntax documentation.

# mshv-all.c
mshv_start_vcpu_thread(const char* thread, uint32_t cpu) "thread %s cpu_index %d"
mshv_handle_interrupt(uint32_t cpu, int mask) "cpu_index %d mask %x"
mshv_set_memory(bool add, uint64_t gpa, uint64_t size, uint64_t user_addr, bool readonly, int ret) "[add = %d] gpa = %lx size = %lx user = %lx readonly = %d result = %d"
mshv_mem_ioeventfd_add(uint64_t addr, uint32_t size, uint32_t data) "addr %lx size %d data %x"
mshv_mem_ioeventfd_del(uint64_t addr, uint32_t size, uint32_t data) "addr %lx size %d data %x"
mshv_shutdown(uint32_t cpu) "cpu %d"

# mgns
mgns_init_vm_db(void) "init vm db"
mgns_update_vm_db(int fd) "update vm db entry for fd %d"
mgns_get_vm_from_db(int fd) "get vm from db for fd %d"

mgns_hvcall_args(const char* hvcall, uint16_t code, uint16_t in_sz) "built args for '%s' code: %d in_sz: %d"

mgns_set_msi_routing(uint32_t gsi, uint64_t addr, uint32_t data) "update msi routing for gsi %d addr %lx data %x"
mgns_remove_msi_routing(uint32_t gsi) "remove msi routing for gsi %d"
mgns_add_msi_routing(uint64_t addr, uint32_t data) "add msi routing for addr %lx data %x"
mgns_enable_msi_routing(int vm_fd) "enable msi routing for vm_fd %d"
mgns_register_irqfd(int vm_fd, int event_fd, uint32_t gsi) "register irqfd for vm_fd %d event_fd %d gsi %d"
mshv_irqchip_update_irqfd_notifier_gsi(int event_fd, int resample_fd, int virq, bool add) "update irqfd notifier gsi for event_fd %d resample_fd %d virq %d add %d"
